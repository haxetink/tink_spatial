	// @formatter:on
	public function wkb2wkt(wkb:String, wkt:String) {
		var parsed:Dynamic = switch Parser.wkb(hex(wkb)) {
			case D2(v): v.toWkt();
			case D3(v): v.toWkt();
		}
		asserts.assert(parsed == wkt);
		return asserts.done();
	}

	function hex(s:String) {
		s = s.replace(' ', '');
		var len = s.length >> 1;
		var bytes = Bytes.alloc(len);
		for (i in 0...len)
			bytes.set(i, Std.parseInt('0x' + s.substr(i * 2, 2)));
		return bytes;
	}
