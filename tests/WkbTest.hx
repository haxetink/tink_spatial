	// @formatter:on
	public function wkb2geojson(wkb:String, geojson:String) {
		var parsed:Dynamic = switch Parser.wkb(hex(wkb)) {
			case D2(v): v.toGeoJson();
			case D3(v): v.toGeoJson();
		}
		var geojson = Json.parse(geojson);
		asserts.compare(geojson, parsed);
		return asserts.done();
	}

